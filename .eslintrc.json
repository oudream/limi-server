{
  "extends": "Google",
  "plugins": [
    "standard",
    "promise"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "indent":["error", 4],
     "semi": 1,
    "quotes": [
      "error",
      "single"
    ],
    "max-len": ["error", { "code": 160 }],
    "switch-colon-spacing": 0
  }
}